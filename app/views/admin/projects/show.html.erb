This is the admin project show page
<br/>
<br/>
<%= @project.name %><br/>
<%= @project.description %><br/>
<%= @project.organization %><br/>
<%= @project.license %><br/>

<br/>
<% @project.networks.each do |network| %>
	<%= link_to network.account_url, network.account_url %><br/>
<% end %>

<br/><br/>

<% if @project.programming_languages.present? %>
	Language tags:<br/>
	<% @project.programming_languages.each do |lang| %>
		<%= lang.language %><br/>
	<% end %>
<% end %>

<br/> <br/>
Project members:<br/>
<% @project.memberships.each do |member| %>
	<% if member.role == "creator" %>
		Project Creator: <%= link_to_users_profile(member.user) %><br/>
	<% else %>
		<%= link_to_users_profile(member.user) %><br/>
	<% end %>
<% end %>
<br/>

