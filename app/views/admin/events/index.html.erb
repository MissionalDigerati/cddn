<table class="table table-striped table-bordered">
	<td>Title</td>
	<td>Details</td>
	<td>Address 1</td>
	<td>Address 2</td>
	<td>City</td>
	<td>State</td>
	<td>Country</td>
	<td>Zip Code</td>
	<td>Online event</td>
	<td>Created at</td>
	<td>Created by</td>
	<td>Action</td>

<% @events.each do |event| %>
<tr>
	<td><%= event.title %></td>
	<td><%= event.details %></td>
	<td><%= event.address_1 %></td>
	<td><%= event.address_2 %></td>
	<td><%= event.city_province %></td>
	<td><%= event.state_id %></td>
	<td><%= event.country_id %></td>
	<td><%= event.zip_code %></td>
	<td><%= event.online_event %></td>
	<td><%= event.created_at %></td>
	<td><%= Event.event_create_by(event) %></td>
	<td><%= link_to "Delete Event", admin_event_path(event), method: :delete, class: "btn btn-danger btn-mini", confirm: "Are you sure?" %></td>
</tr>
<% end %>
</table>