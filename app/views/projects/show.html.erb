This is the project show page
<br/>
<br/>
<%= @project.name %><br/>
<%= @project.description %><br/>
<%= @project.organization %><br/>
<%= @project.license %><br/>

<br/>
<% @project.networks.each do |network| %>
	<%= link_to network.account_url, network.account_url %><br/>
<% end %>

<br/><br/>

Language tags:<br/>
<% display_programming_tags(@project.programmings.pluck(:programming_language_id)).each do |language|%>
	<%= language %> <br/>
<% end %>
<br/> <br/>
Project members:<br/>
<% @project.memberships.each do |member| %>
	<% if member.role == "creator" && member.team_approved == true %>
		Project Creator: <%= link_to_users_profile(member.user) %><br/>
	<% elsif member.role != "creator" && member.team_approved == true %>
		<%= link_to_users_profile(member.user) %><br/>
	<% end %>
<% end %>
<br/>

<%= join_project_request_button(@project, current_user) if user_signed_in?%>

