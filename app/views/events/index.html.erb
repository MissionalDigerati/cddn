Listing all events
<table class="table table-striped table-bordered">
	<tr>
		<td>Title</td>
		<td>Details</td>
		<td>City</td>
		<td>State</td>
		<td>Country</td>
		<td>Online Event</td>
		<td>Date</td>
		<td>Action</td>
	</tr>
	<% @events.each do |event| %>
	</tr>
		<td><%= event.title %></td>
		<td><%= event.details %></td>
		<td><%= event.city_province %></td>
		<td><%= event.state_id %></td>
		<td><%= event.country_id %></td>
		<td><%= event.online_event %></td>
		<td><%= event.event_date %></td>
		<td><%= link_to "View Event", event_path(event), class: "btn btn-mini btn-info" %> <%= attend_event_button(current_user.id, event) if user_signed_in? %></td>
	</tr>
	<% end %>
</table>