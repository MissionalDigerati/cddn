<div class="container">
	Listing past events
	<br/>
	<br/><br/>
	<table id="events" class="table table-striped table-bordered">
		<tbody>
		<tr>
			<td>Title</td>
			<td>City/Province</td>
			<td>State</td>
			<td>Country</td>
			<td>Online Event</td>
			<td>Date (m/d/y)</td>
			<td>Action</td>
		</tr>
			<%= render @events %>
		</tbody>
	</table>
	<%= paginate @events, :theme => 'twitter-bootstrap' %>
</div>